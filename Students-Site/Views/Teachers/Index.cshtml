@model System.Collections.Generic.IEnumerable<Students_Site.Models.TeacherModel>

@{
	ViewBag.Title = "Учителя";
	Layout = "_Layout";
}

<h1>Учителя</h1>

<table class="table">
	<tr>
		<th>
			Имя
		</th>
		<th>
			Фамилия
		</th>
		<th>
			Предмет
		</th>
		<th></th>
	</tr>

	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.FirstName)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.LastName)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.SubjectName)
			</td>
		</tr>
	}
</table>
